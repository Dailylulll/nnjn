    "import numpy as np\n",
    "\n",
    "# Coefficient matrix A\n",
    "A = np.array([[0, 1, 4], [1, 3, 3], [3, 7, 5]])\n",
    "\n",
    "# Ordinate or \"dependent variable\" values b\n",
    "b = np.array([-4, -2, 6])\n",
    "\n",
    "# Solve the linear system Ax = b\n",
    "x = np.linalg.solve(A, b)\n",
    "\n",
    "print(x)  # Output will be the solution of the system"
   ]
